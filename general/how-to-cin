//using vector like a map
vector<pair<char, int> > c;
string s = "11122333";
for(auto x : s)
{
	if(c.empty() || c.back().first != x)
		c.push_back(make_pair(x, 1));
	else
		c.back().second++;
}




//how to find elements in map<int,int> 
