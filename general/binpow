#include <bits/stdc++.h>

using u64 = uint64_t;
using u128 = __uint128_t;
using ll = long long;
u64 binpower(u64 base, u64 e, u64 mod) {
    //integer range [2 <= N <= 2^63-1] can be AC
    u64 result = 1;
    base %= mod;
    while (e) {
        if (e & 1){
            result = (u128)result * base % mod;
        }
        base = (u128)base * base % mod;
        e >>= 1;
    }
    return result;
}


ll binpower(ll a, ll b, ll mod) {
    a %= mod;
    ll res = 1;
    while (b > 0) {
        if (b & 1)
            res = res * a % mod;
        a = a * a % mod;
        b >>= 1;
    }
    return res;
}

